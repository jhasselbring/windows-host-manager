{
  "name": "host-manager",
  "description": "Desktop app that allows you to easily manage hosts.",
  "version": "0.1.6",
  "private": true,
  "author": "ToolBox Plus",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:publish": "vue-cli-service electron:build -p always",
    "electron:serve": "vue-cli-service electron:serve --enable-transparent-visuals",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "main": "background.js",
  "dependencies": {
    "browser-window": "^0.4.0",
    "core-js": "^3.6.5",
    "electron-updater": "^4.3.8",
    "lodash": "^4.17.20",
    "remote": "^0.2.6",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "electron": "^11.1.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^2.0.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5"
  }
}
